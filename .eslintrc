{
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier"
    ],
    "parserOptions": { "project": "./tsconfig.json", "ecmaVersion": 2021, "sourceType": "module" },
    "rules": {      
      "prettier/prettier": 2,
      "import/no-import-module-exports" : "off"
    },
      "env": {
        "es2021": true,
        "browser": true,
        "jest": true,
        "node": true
      }
  }